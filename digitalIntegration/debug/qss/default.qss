}
/* 隐藏默认关闭按钮 */
QTabBar::close-button {
    subcontrol-position: right;
    subcontrol-origin: padding;
    width: 0px;
    height: 0px;
}

/* 设置自定义关闭按钮图标 */
QTabBar::close-button {
    image: url(:/image/tarBar_close.png); /* 默认图标 */
}

QTabBar::close-button:hover {
            image: url(:/image/tarBar_close.png);
        }
	
