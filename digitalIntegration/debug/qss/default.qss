}

/* ------------------------ 通用按钮样式----------------------start*/

QPushButton#btnData,QPushButton#btnUser,QPushButton#btnFile,QPushButton#btnDownload{
		color:#7482a6;
		font-size: 22px;
		border: none;
}

QPushButton#btnData:pressed,QPushButton#btnUser:pressed,QPushButton#btnFile:pressed,QPushButton#btnDownload:pressed{
		color:ffffff;
